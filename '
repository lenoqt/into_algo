from typing import List

input_n = input('Insert integer numbers separated by a space')
numbers : List[int] = [int(x) for x in input_n.split()]

def insert_sort(numbers:List):
    if len(numbers) <= 1:
        return numbers
    sorted_num : List[int] = []
    for j in range(1, len(numbers)):
        key = numbers[j]
        # Insert numbers[j] into sorted_num[1...j-1]
        i = j -1
        while i > 0 and sorted_num[i] > key:
            sorted_num[i + 1] = sorted_num[i]
            i =- 1
        sorted_num[i + 1] = key

    return sorted_num
        


if __name__ == "__main__":
    insert_sort(numbers)
